#!/usr/bin/env ruby

require_relative '../lib/move.rb'

index = gets.strip
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
if input_to_index(index) == even?
  token = "O"
else
  token = "X"
end

puts "Welcome to Tic Tac Toe!"
input_to_index(index)
move(board, index, token)
# Code your CLI Here
